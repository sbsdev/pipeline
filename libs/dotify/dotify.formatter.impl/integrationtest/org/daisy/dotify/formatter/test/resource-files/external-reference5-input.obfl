<?xml version="1.0" encoding="UTF-8"?>
<obfl xmlns="http://www.daisy.org/ns/2011/obfl" version="2011-1" xml:lang="und" hyphenate="false">
   <meta xmlns:dc="http://purl.org/dc/elements/1.1/">
      <dc:title>Tests external-reference element</dc:title>
      <dc:description>Tests two external-reference attributes in the same document with the same
      prefix but different namespace URI.</dc:description>
   </meta>
   <layout-master name="master" page-width="10" page-height="5" duplex="true">
      <default-template>
         <header/>
         <footer/>
      </default-template>
   </layout-master>
   <sequence master="master">
      <block xmlns:example="http://example1.com">
        <external-reference example:id="exampleValue"/>⠁⠁⠁
      </block>
      <block xmlns:example="http://example2.com">
        <external-reference example:id="exampleValue"/>⠁⠁⠁
      </block>
   </sequence>
</obfl>
