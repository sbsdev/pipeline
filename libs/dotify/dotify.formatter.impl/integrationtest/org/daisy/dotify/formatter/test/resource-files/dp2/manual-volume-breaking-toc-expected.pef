<?xml version="1.0" encoding="UTF-8"?>
<pef xmlns="http://www.daisy.org/ns/2008/pef" version="2008-1">
   <head>
      <meta>
         <dc:title xmlns:dc="http://purl.org/dc/elements/1.1/">manual-volume-breaking-toc</dc:title>
         <dc:description xmlns:dc="http://purl.org/dc/elements/1.1/">Tests manual volume breaking in combination with table of contents.</dc:description>
         <dc:format xmlns:dc="http://purl.org/dc/elements/1.1/">application/x-pef+xml</dc:format>
      </meta>
   </head>
   <body>
      <volume cols="15" rows="15" rowgap="0" duplex="true">
         <section>
            <page>
               <row>⠼⠁</row>
            </page>
         </section>
         <section>
            <page>
               <row>⠿⠿⠿⠿⠿</row>
            </page>
         </section>
      </volume>
      <volume cols="15" rows="15" rowgap="0" duplex="true">
         <section>
            <page>
               <row>⠼⠉</row>
            </page>
         </section>
         <section>
            <page>
               <row>⠿⠿⠿⠿⠿</row>
            </page>
         </section>
      </volume>
   </body>
</pef>