	  <speak version="1.1">
	    <s id="s1" xml:lang="en">
	      <prosody volume="2">
		<prosody pitch="0">
		  <prosody rate="3">
		    <token>sentence</token>
		  </prosody>
		</prosody>
	      </prosody>
	    </s>
	  </speak>
	</x:document>
      </x:expect>
    </x:scenario>

