    stylesheet="../../main/resources/xml/xslt/zedai-to-html.xsl">
    <x:scenario label="Simple MathML">
        <x:context>
            <m:math alttext="cube root of x">
                <m:mroot>
                    <m:mi>x</m:mi>
                    <m:mn>3</m:mn>
                </m:mroot>
            </m:math>
        </x:context>
        <x:expect label="should be copied as-is">
            <m:math alttext="cube root of x">
                <m:mroot>
                    <m:mi>x</m:mi>
                    <m:mn>3</m:mn>
                </m:mroot>
            </m:math>
        </x:expect>
    </x:scenario>
</x:description>
