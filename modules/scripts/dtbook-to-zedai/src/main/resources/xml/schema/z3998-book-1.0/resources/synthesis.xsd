          </xs:union>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="rate">
        <xs:simpleType>
          <xs:union>
            <xs:simpleType>
              <xs:restriction base="xs:token">
                <xs:enumeration value="x-slow"/>
              </xs:restriction>
            </xs:simpleType>
            <xs:simpleType>
