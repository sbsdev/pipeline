                        </optional>
                        <optional>
                            <ref name="xlink.title.attrib"/>
                        </optional>
                        <optional>
                            <ref name="xlink.show.attrib"/>
                        </optional>
                        <optional>
                            <ref name="xlink.actuate.attrib"/>
                        </optional>
                    </interleave>
                </optional>
                <optional>
                    <ref name="z3998.ref.attrib"/>
                </optional>
            </choice>
