            </optional>
            <optional>
                <ref name="rdfa.datatype.attrib"/>
            </optional>
            <optional>
                <ref name="rdfa.typeof.attrib"/>
            </optional>
            <optional>
                <ref name="rdfa.property.attrib"/>
            </optional>
            <optional>
                <ref name="rdfa.rel.attrib"/>
            </optional>
            <optional>
                <ref name="rdfa.resource.attrib"/>
            </optional>
            <optional>
                <ref name="rdfa.rev.attrib"/>
            </optional>
            <optional>
                <ref name="z3998.desc.attrib"/>
            </optional>
            <optional>
                <ref name="z3998.by.attrib"/>
            </optional>
            <optional>
                <ref name="z3998.select.selid.attrib"/>
            </optional>
            <optional>
                <ref name="z3998.role.attrib"/>
            </optional>
            <optional>
                <ref name="ssml.onlangfailure.attrib"/>
            </optional>
            <oneOrMore>
                <choice>
                    <ref name="z3998.meta"/>
                    <ref name="z3998.description"/>
                    <ref name="z3998.expansion"/>
                    <ref name="z3998.definition"/>
                    <ref name="ssml.lexicon"/>
                    <ref name="xforms.model"/>
                </choice>
            </oneOrMore>
        </element>
    </define>
    <define name="z3998.body">
        <element name="body" ns="http://www.daisy.org/ns/z3998/authoring/">
