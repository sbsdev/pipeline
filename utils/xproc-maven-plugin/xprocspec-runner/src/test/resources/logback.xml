<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[XPROCSPEC] %level: %msg%n</pattern>
    </encoder>
  </appender>
  <root level="OFF">
    <appender-ref ref="STDOUT"/>
  </root>
  <logger name="org.daisy.xprocspec" level="WARN"/>
</configuration>
