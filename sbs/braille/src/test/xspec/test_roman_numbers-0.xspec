    <x:scenario label="Roman number, http://redmine.sbszh.ch/issues/2634">
      <x:context>
	<dtb:p xml:lang="de">Seite <brl:num role="roman">VI</brl:num></dtb:p>
      </x:context>
      <x:expect label="braille">
	<dtb:p xml:lang="de">SEITE <brl:num role="roman">&gt;VI</brl:num></dtb:p>
      </x:expect>
    </x:scenario>

