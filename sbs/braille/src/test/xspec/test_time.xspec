<?xml version="1.0" encoding="utf-8"?>
<x:description xmlns:x="http://www.jenitennison.com/xslt/xspec"
	       xmlns:dtb="http://www.daisy.org/z3986/2005/dtbook/"
	       xmlns:brl="http://www.daisy.org/z3986/2009/braille/"
               stylesheet="../resources/block-translate.xsl"
	       preserve-space="dtb:p dtb:byline dtb:author dtb:li dtb:lic dtb:doctitle dtb:docauthor dtb:span dtb:em dtb:strong brl:emph dtb:line dtb:h1 dtb:h2 dtb:h3 dtb:h4 dtb:h5 dtb:h6">

  <x:param name="contraction-grade">2</x:param>
  <x:param name="ascii-braille">yes</x:param>

    <x:scenario label="time compact">
      <x:context>
        <brl:time xml:lang="de" brl:render="compact" value="09:30">09:30</brl:time>
      </x:context>
      <x:expect label="braille">
        <brl:time xml:lang="de" brl:render="compact" value="09:30">#I.CJ</brl:time>
      </x:expect>
    </x:scenario>

    <x:scenario label="time ">
      <x:context>
        <brl:time xml:lang="de" value="09:30">09:30</brl:time>
      </x:context>
      <x:expect label="braille">
        <brl:time xml:lang="de" value="09:30">#JI':#CJ</brl:time>
      </x:expect>
    </x:scenario>

</x:description>
